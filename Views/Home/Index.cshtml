@{
    ViewData["Title"] = "Home Page";
}
@model BowlingLeague.Models.ViewModels.IndexViewModel



<div class="text-center">
    <h2>@Model.TeamName</h2>   
</div>



<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Middle Init</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone Number</th>
            
        </tr>
    </thead>
    <tbody>
   <!--iterate the list-->

        @foreach (var x in Model.Bowlers)
        {
            <tr>
                <td>@x.BowlerFirstName</td>
                <td>@x.BowlerMiddleInit</td>
                <td>@x.BowlerLastName</td>
                <td>@x.BowlerAddress</td>
                <td>@x.BowlerCity</td>
                <td>@x.BowlerState</td>
                <td>@x.BowlerZip</td>
                <td>@x.BowlerPhoneNumber</td>
            </tr>
        }
    </tbody>

</table>

<div page-info="@Model.PageNumberingInfo" page-url-teamname="@Model.TeamName"></div>